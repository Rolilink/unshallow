{
  "name": "unshallow",
  "version": "0.1.0",
  "description": "CLI tool for migrating Enzyme tests to React Testing Library",
  "main": "dist/index.js",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@langchain/community": "^0.3.39",
    "@langchain/core": "^0.3.43",
    "@langchain/openai": "^0.5.2",
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "ink": "^5.2.0",
    "langchain": "^0.3.19",
    "langsmith": "^0.3.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.2"
  }
}
